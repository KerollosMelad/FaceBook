@model FaceBook_InitialVersion.Models.PersonModelView
@using Microsoft.AspNetCore.Identity
@inject SignInManager<Person> SignInManager
@inject UserManager<Person> UserManager
@{
    ViewData["Title"] = "Profile";
}

@*Link to Profile & (edit info if you are the current user else add friend/remove friend )  *@
<div class="theme-layout">

    <section>
        <div class="feature-photo">
            <figure>  <img src="~/images/timeline-1.jpeg" alt="can't load cover photo" /></figure>
            <div class="add-btn">
                @{
                    <span style="    top: -83px;
                        right: 20px;
                        position: relative;
                        float: right;">1205 followers</span>
                    if ((SignInManager.IsSignedIn(User)) && !(User.Identity.Name == @Model.CurrentUser.UserName))
                    {
                        // login person => User.Identity.Name
                        //@Model now is Map To friend
                        if (@Model.myFriends.Any(F => F.UserName == User.Identity.Name))
                        {
                            <span id="friendshipState" style="    float: right;">
                                <partial name="_RemoveFriendFromProfilePartial" model="@Model.CurrentUser" />
                            </span>
                        }
                        else if (@Model.CurrentUser.FriendsRequest.Any(F => F.User.UserName == User.Identity.Name))
                        {
                            <div id="friendshipState">
                                @*// friend sent friend request to you*@
                                <partial name="_DeleteRequestToFriendPartial" model="@Model.CurrentUser" />
                            </div>
                        }
                        else if (@Model.CurrentUser.MyRequests.Any(F => F.Friend.UserName == User.Identity.Name))
                        {
                            <!--<div id="friendshipState">
                                @*// you sent the friend request to him*@
                                <partial name="_ConfirmDeleteFriendRequestPartial" model="@Model.CurrentUser" />
                            </div>-->
                        }
                        else
                        {
                            <div id="friendshipState">
                                <partial name="_AddFriendPartial" model="@Model.CurrentUser" />
                            </div>
                        }
                    }

                }
            </div>
            <form class="edit-phto">
                <i class="fa fa-camera-retro"></i>
